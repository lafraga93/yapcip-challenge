{
    "name": "lafraga93/yapcip-challenge",
    "description": "A simple RESTFul API that simulates a money transactions between users",
    "keywords": ["lafraga93", "challenge"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "~7.4",
        "fakerphp/faker": "^1.13",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/lumen-framework": "~8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
